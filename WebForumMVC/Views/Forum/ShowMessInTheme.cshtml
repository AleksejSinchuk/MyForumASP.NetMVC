
@{
    ViewBag.Title = "Mwssages";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@foreach (var item in  ViewBag.msg)
{


    <table class="tbl">
        <tr>
            <td class="td1" style="width:60vw;">@item.Msg</td>
            <td class="td1" style="width: 15vw;">Автор</td>
            <td class="td1" style="width:25vw;">Дата создания <div><button style="background:red;color:greenyellow;">Удалить</button></div></td>
        </tr>
    </table>
}